{% extends 'base.html.twig' %}
{% block title %}
    Create a new record | {{ parent() }}
    {# parent() is function to get parent title#}
{% endblock %}
{% block body %}
    

    <h1>
    {{ title }}</h1>

    {# for comment #}

    <div>
    Tracks:
    <ul>
        {% for track in tracks %}
            <li>{{ track.song }} - {{ track.artist|upper }}</li>
        {% endfor %}
    </ul>
    </div>
{% endblock %}